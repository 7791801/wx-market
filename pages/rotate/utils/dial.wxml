<template name="dial" >
  <view class="dial_wp">
    <image src = "./utils/images/dial_bg.png" style="-webkit-transform:rotate({{singleAngle/2}}deg) translateZ(0);transform:rotate({{singleAngle/2}}deg) translateZ(0)"/>
    <view class="dial_pointer" style="-webkit-transform:rotate({{deg}}deg) translateZ(0);transform:rotate({{deg}}deg) translateZ(0)" bindtap="start"><image src = "./utils/images/dial_pointer.png" /></view>
  </view>
</template> 